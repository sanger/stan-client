    "@types/jest": "^24.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/yup": "^0.29.8",
    "autoprefixer": "^9.0.0",
    "classnames": "^2.2.6",
    "formik": "^2.2.0",
    "graphql": "^15.3.0",
