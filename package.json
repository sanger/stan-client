    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-transition-group": "^4.4.1",
    "tailwindcss": "^1.8.10",
    "typescript": "~3.7.2",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "start:msw": "REACT_APP_MOCK_API=msw PORT=3001 npm run start",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/styles/tailwind.css -o src/styles/index.css -w",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/index.css --env production",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "start-server-and-test start:msw 3001 cypress:run",
    "test:open": "start-server-and-test start:msw 3001 cypress:open",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.18.0",
    "@testing-library/cypress": "^7.0.1",
    "cypress": "^5.3.0",
    "cypress-intellij-reporter": "^0.0.4",
    "msw": "^0.21.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "prettier": "2.1.2",
    "start-server-and-test": "^1.11.5",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-react-apollo": "2.0.7"
  }
